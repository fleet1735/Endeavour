{
    "strategy_name": "Double Bottom Recovery v2 (for Rollback)",
    "strategy_version": "2.0",
    "author": "Commander",
    "description": "v3.5 코드와 호환되는 이전 버전 전략 파일입니다.",
    "universe_filters": [
        {
            "filter_type": "market_cap",
            "min_market_cap": 1000000000000
        },
        {
            "filter_type": "trading_value",
            "min_value": 10000000000,
            "days": 20
        }
    ],
    "indicators": [
        {
            "id": "SMA_5_Close",
            "type": "SMA",
            "params": {
                "period": 5,
                "source": "Close"
            }
        },
        {
            "id": "SMA_20_Close",
            "type": "SMA",
            "params": {
                "period": 20,
                "source": "Close"
            }
        },
        {
            "id": "MIN_60_Low",
            "type": "MIN",
            "params": {
                "period": 60,
                "source": "Low"
            }
        }
    ],
    "entry_rules": {
        "logic": "AND",
        "conditions": [
            {
                "op": ">",
                "left": "Close",
                "right": "MIN_60_Low"
            },
            {
                "op": "crosses_above",
                "left": "SMA_5_Close",
                "right": "SMA_20_Close"
            }
        ]
    },
    "exit_rules": {
        "logic": "OR",
        "conditions": [
            {
                "op": "crosses_below",
                "left": "SMA_5_Close",
                "right": "SMA_20_Close"
            }
        ]
    }
}