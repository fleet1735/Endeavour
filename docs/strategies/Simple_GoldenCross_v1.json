{
  "strategy_name": "Simple_GoldenCross_5_20_SL3_TP5_v2",
  "author": "Commander & Gemini",
  "description": "[v2 호환성 테스트] 단순 골든크로스. 5일선이 20일선 상향 돌파 시 매수. 손절 3%, 익절 5%.",
  "universe_filters": { "exclude": [] },
  "indicators": [
    { "name": "SMA5", "type": "SMA", "params": { "source": "Close", "window": 5 }},
    { "name": "SMA20", "type": "SMA", "params": { "source": "Close", "window": 20 }}
  ],
  "entry_rules": {
    "initial_entry": {
      "name": "GoldenCross_Entry",
      "logic": "AND",
      "conditions": [
        { "left": "SMA5", "op": "crosses_above", "right": "SMA20" }
      ]
    }
  },
  "exit_rules": {
    "take_profit": {
      "name": "Take_Profit_5_Percent",
      "logic": "OR",
      "conditions": [
        { "left": "Close", "op": ">=", "right": "average_entry_price * 1.05" }
      ]
    },
    "stop_loss": {
      "name": "Stop_Loss_3_Percent",
      "logic": "OR",
      "conditions": [
        { "left": "Close", "op": "<=", "right": "average_entry_price * 0.97" }
      ]
    }
  },
  "optimization_config": { "enabled": false }
}